generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  IdUsuario Int    @id @default(autoincrement())
  Nombre    String
  Apellido  String
  Mail      String
}

model Ejercicios {
  Idejercicios Int     @id @unique @default(autoincrement())
  Nombre       String
  Descripcion  String
  Alternative  String?
}

model Rutinas {
  Idrutinas    Int @id @default(autoincrement())
  Idejercicios Int
}
